            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/abstract_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/products_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/domain_builder_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/abstract_ui_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/dialogs/dialog_product_selector.js"></script>
