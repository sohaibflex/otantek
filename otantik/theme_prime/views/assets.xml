            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/ui_configurator_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/category_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/category_ui_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/collection_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/collection_ui_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/hierarchical_category_widget.js"></script>
            <script type="text/javascript" src="/theme_prime/static/src/js/editor/widgets/hierarchical_category_ui_widget.js"></script>
        </xpath>
    </template>

