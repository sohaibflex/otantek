            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/website_sale.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/icon-packs/website.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/utils.scss"/>
