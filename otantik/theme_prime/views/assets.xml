    <template id="website_assets_wysiwyg" inherit_id="website.assets_wysiwyg">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/icon-packs/wysiwyg.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/icon-packs/website.edit_mode.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/editor/editor.scss"/>
            <!-- Widgets -->
            <link rel="stylesheet" type ="text/scss" href="/theme_prime/static/src/scss/editor/widgets/products_widget.scss"/>
            <link rel="stylesheet" type ="text/scss" href="/theme_prime/static/src/scss/editor/widgets/domain_builder.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_prime/static/src/scss/editor/widgets/select2_widget.scss"/>
